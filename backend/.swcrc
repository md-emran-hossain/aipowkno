{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true
    },
    "transform": {
      "decoratorMetadata": true
    },
    "target": "es2020"
  },
  "module": {
    "type": "es6",
    "strict": true,
    "lazy": false,
    "noInterop": false
  },
  "sourceMaps": true
}
